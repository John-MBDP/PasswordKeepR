    <section class="section-cta" id="cta">
      <div class="container">
        <div class="cta">
          <div class="cta-text-box">
            <h2 class="heading-secondary">Generate Password</h2>
            <form class="cta-form" name="sign-up" netlify>
              <div>
                <label for="Password-Length">Password Length</label>
                <input
                  id="length"
                  type="number"
                  min="4"
                  max="20"
                />
              </div>
              <div>
              </div>
              <div>
                <label for="Include-Uppercase">Include uppercase letters</label>
                <input class="cta-input"
                  id="uppercase"
                  type="checkbox"
                  checked
                />
                <label for="Include-Lowercase">Include Lowercase letters</label>
                <input class="cta-input"
                  id="lowercase"
                  type="checkbox"
                  checked
                />
                <label for="Include-Numbers">Include Numbers</label>
                <input class="cta-input"
                  id="numbers"
                  type="checkbox"
                  checked
                />

                <label for="Include-Symbols">Include Symbols</label>
                <input class="cta-input"
                  id="symbols"
                  type="checkbox"
                  checked
                />
                <label for="Password-Length">Results</label>
                <input
                  id="result"
                />
              </div>
                <button class="btn btn--form">Generate</button>

            </form>
          </div>
        </div>
      </div>
